{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-alert": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "arrow-parens": [
      2,
      "always"
    ],
    "no-param-reassign": "off",
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
    "globals": {
      "cy": true
    }
  
}
